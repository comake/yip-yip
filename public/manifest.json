{
  "name": "YipYip",
  "short_name": "YipYip",
  "version": "1.0.0",
  "description": "Super fast browsing",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["/static/css/content.css"],
      "js": ["/static/js/content.js"],
      "all_frames": false,
      "run_at": "document_idle"
    }
  ]
}
