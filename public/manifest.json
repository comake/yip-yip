{
  "name": "YipYip",
  "short_name": "YipYip",
  "version": "1.1.0",
  "description": "Super fast browsing",
  "manifest_version": 3,
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["/static/css/content.css"],
      "js": ["/static/js/content.js"],
      "all_frames": false,
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_icon": {
      "16": "logo-16.png",
      "24": "logo-24.png",
      "32": "logo-32.png",
      "48": "logo-48.png",
      "64": "logo-64.png",
      "128": "logo-128.png"
    },
    "default_title": "Search with YipYip!"
  },
  "icons": {
    "16": "logo-16.png",
    "24": "logo-24.png",
    "32": "logo-32.png",
    "48": "logo-48.png",
    "64": "logo-64.png",
    "128": "logo-128.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": [
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "*://*/*"
  ]
}
