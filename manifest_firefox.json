{
  "manifest_version": 2,
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "logo-32.png",
    "default_title": "Search with YipYip!"
  },
  "permissions": [
    "storage",
    "<all_urls>"
  ]
}
